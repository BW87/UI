<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="347dp"
    android:layout_height="92dp">

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginTop="19dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ico_announcement_gray_24_px" />

    <TextView
        android:id="@+id/alarm_history_announce_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/noto_sans_cj_kkr_bold"
        android:text="TextView"
        android:textStyle="bold"
        android:textSize="20sp"
        android:textColor="#444444"
        android:letterSpacing="-0.03"
        android:lineSpacingExtra="4sp"
        app:layout_constraintStart_toEndOf="@+id/imageView10"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/alarm_history_announce_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="42dp"
        android:fontFamily="@font/noto_sans_cj_kkr_regular"
        android:letterSpacing="-0.045"
        android:text="TextView"
        android:textColor="#222222"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/alarm_history_risk_go_button"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:background="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ico_setting_arrow_right_bk" />

    <ImageView
    android:id="@+id/imageView11"
    android:layout_width="match_parent"
    android:layout_height="1dp"
    app:layout_constraintBottom_toBottomOf="parent"
    android:background="#eaeaea"
    />
</androidx.constraintlayout.widget.ConstraintLayout>